#include <vector>
class Solution {
public:
    int heightChecker(vector<int>& heights) {
        int res = 0;
        int curHeight = 0;
        vector<int> freq(101, 0); 

        for (int height : heights) {
            freq[height]++;
        }

        for (int i = 0; i < heights.size(); i++) {
            while (freq[curHeight] == 0) 
                curHeight++; 

            if (curHeight != heights[i]) 
                res++;

            freq[curHeight]--;
        }

        return res;
    }
};
